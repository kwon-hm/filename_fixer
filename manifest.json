{
  "manifest_version": 3,
  "name": "맥에서 보낸 첨부파일이 자음/모음 분리되어 깨진 한글 파일명 복원기",
  "version": "1.0.0",
  "description": "자음/모음이 분리된 깨진 한글 파일명을 정상 한글로 자동 복원",
  "permissions": ["downloads"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    }
  },
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  }
}
